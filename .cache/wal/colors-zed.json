                      "color": "#9cbd8a"
                    },
                    "function": {
                      "color": "#78bcc1"
                    },
                    "keyword": {
                      "color": "#bd8a99"
                    },
                    "number": {
                      "color": "#9578c1"
                    },
                    "operator": {
                      "color": "#bd8a99"
                    },
                    "string": {
                      "color": "#7897c1"
                    },
                    "variable": {
                      "color": "#9cbd8a"
                    }
                },
                "players": [
                  {
                    "cursor": "#7897c1",
