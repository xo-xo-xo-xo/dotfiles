                "terminal.ansi.yellow": "#c1a978",
                "terminal.ansi.blue": "#bd8a99",
                "terminal.ansi.magenta": "#9578c1",
                "terminal.ansi.cyan": "#78bcc1",
                "terminal.ansi.white": "#95aec7",
                "terminal.ansi.bright_black": "#171711",
